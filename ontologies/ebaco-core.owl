        <rdfs:label xml:lang="es">Región vinícola de España con denominación de origen</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ebaco;Spanish_wine_region_with_quality_denomination"/>
        <rdfs:seeAlso rdf:resource="http://www.winesfromspain.com/icex/cda/controller/pageGen/0,3346,1559872_6763376_6792160_1058_0,00.html"/>
    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_with_protected_geographical_indicator -->

