    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_with_denomination_of_pago -->

    <owl:Class rdf:about="&ebaco;Spanish_wine_region_with_denomination_of_pago">
        <rdfs:label xml:lang="en">Spanish wine region with denomination of pago</rdfs:label>
        <rdfs:label xml:lang="es">Región vinícola de España de Pago</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ebaco;Spanish_wine_region_with_quality_denomination"/>
    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_with_protected_denomination_of_origin -->

    <owl:Class rdf:about="&ebaco;Spanish_wine_region_with_protected_denomination_of_origin">
