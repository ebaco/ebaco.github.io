        <rdfs:comment xml:lang="es">Vino espumso sin ningún licor de expedición añadido.</rdfs:comment>
    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spakling_wine_extra_brut -->

    <owl:Class rdf:about="&ebaco;Spakling_wine_extra_brut">
        <rdfs:label xml:lang="en">Spakling wine extra brut</rdfs:label>
        <rdfs:label xml:lang="es">Vino espumoso extra brut</rdfs:label>
