        <rdfs:label xml:lang="es">Región vinícola de España con denominación de origen calificada</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ebaco;Spanish_wine_region_with_quality_denomination"/>
    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_with_quality_denomination -->

    <owl:Class rdf:about="&ebaco;Spanish_wine_region_with_quality_denomination">
        <rdfs:label xml:lang="en">Spanish wine region with quality denomination</rdfs:label>
        <rdfs:label xml:lang="es">Región vinícola de España con denominación de calidad</rdfs:label>
        <rdfs:subClassOf rdf:resource="&ebaco;Spanish_wine_region"/>
        <owl:disjointWith rdf:resource="&ebaco;Spanish_wine_region_without_quality_denomination"/>
        <rdfs:comment xml:lang="es">Región de España donde se producen vinos de calidad que están protegidos por un consejo regulador o alguna normativa.</rdfs:comment>
        <rdfs:seeAlso rdf:resource="http://es.wikipedia.org/wiki/Anexo:Vinos_de_Espa%C3%B1a#Lista_de_Denominaciones_de_Origen_y_Vinos_de_la_Tierra_por_Comunidad_Aut.C3.B3noma"/>
        <rdfs:seeAlso rdf:resource="http://www.winesfromspain.com/icex/cda/controller/pageGen/0,3346,1559872_6763376_6792111_0,00.html"/>
        <owl:disjointUnionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="&ebaco;Spanish_wine_region_with_denomination_of_pago"/>
            <rdf:Description rdf:about="&ebaco;Spanish_wine_region_with_protected_denomination_of_origin"/>
            <rdf:Description rdf:about="&ebaco;Spanish_wine_region_with_protected_geographical_indicator"/>
            <rdf:Description rdf:about="&ebaco;Spanish_wine_region_with_qualified_denomination_of_origin"/>
        </owl:disjointUnionOf>
    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_without_quality_denomination -->

    <owl:Class rdf:about="&ebaco;Spanish_wine_region_without_quality_denomination">
        <rdfs:label xml:lang="en">Spanish wine region without quality denomination</rdfs:label>
        <rdfs:label xml:lang="es">Región vinícola de España sin denominación de calidad</rdfs:label>
