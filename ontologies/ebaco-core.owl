    </owl:Class>
    


    <!-- http://purl.org/ceu/ebaco/core/1.0#Spanish_wine_region_with_qualified_denomination_of_origin -->

    <owl:Class rdf:about="&ebaco;Spanish_wine_region_with_qualified_denomination_of_origin">
