    <!-- http://purl.org/ceu/ebaco/core/1.0#Young_wine -->

    <owl:Class rdf:about="&ebaco;Young_wine">
        <rdfs:label xml:lang="en">Young wine</rdfs:label>
        <rdfs:label xml:lang="es">Vino joven</rdfs:label>
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&ebaco;Wine"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&ebaco;Red_wine"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&ebaco;time_of_aging_in_barrel"/>
                                        <owl:someValuesFrom>
                                            <rdfs:Datatype>
                                                <owl:onDatatype rdf:resource="&xsd;float"/>
                                                <owl:withRestrictions rdf:parseType="Collection">
                                                    <rdf:Description>
                                                        <xsd:maxExclusive rdf:datatype="&xsd;float">6.0</xsd:maxExclusive>
                                                    </rdf:Description>
                                                </owl:withRestrictions>
                                            </rdfs:Datatype>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="&ebaco;White_wine"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="&ebaco;time_of_aging_in_barrel"/>
                                        <owl:someValuesFrom>
                                            <rdfs:Datatype>
                                                <owl:onDatatype rdf:resource="&xsd;float"/>
                                                <owl:withRestrictions rdf:parseType="Collection">
                                                    <rdf:Description>
                                                        <xsd:maxInclusive rdf:datatype="&xsd;float">0.0</xsd:maxInclusive>
                                                    </rdf:Description>
                                                </owl:withRestrictions>
                                            </rdfs:Datatype>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="es">Vino del año sin paso por barrica o con un máximo de seis meses de añejamiento en barrica.</rdfs:comment>
    </owl:Class>
    


